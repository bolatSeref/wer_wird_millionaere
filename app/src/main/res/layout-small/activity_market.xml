<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_gems_purchase"
    tools:context=".MarketActivity">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:onClick="backImage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_arrow_back" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCurrentGem"
        app:srcCompat="@drawable/title_gems_market" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="80dp"
        android:layout_height="24dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/area_points" />

    <TextView
        android:id="@+id/tvCurrentGem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="@string/gems"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView25"
        app:layout_constraintEnd_toEndOf="@+id/imageView25"
        app:layout_constraintStart_toStartOf="@+id/imageView25"
        app:layout_constraintTop_toTopOf="@+id/imageView25" />

    <ImageView
        android:id="@+id/imgGem1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView25"
        app:srcCompat="@drawable/btn_gems_market" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem1"
        app:layout_constraintEnd_toEndOf="@+id/imgGem1"
        app:layout_constraintStart_toStartOf="@+id/imgGem1"
        app:layout_constraintTop_toTopOf="@+id/imgGem1"
        app:srcCompat="@drawable/gems_mar1" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="@string/gems_100"
        android:textColor="#000"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="@+id/imgGem1"
        app:layout_constraintStart_toStartOf="@+id/imgGem1"
        app:layout_constraintTop_toTopOf="@+id/imgGem1" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:fontFamily="@font/regular"
        android:text="@string/tl_4"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem1"
        app:layout_constraintEnd_toEndOf="@+id/imgGem1"
        app:layout_constraintStart_toStartOf="@+id/imgGem1" />

    <ImageView
        android:id="@+id/imgGem2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgGem1"
        app:srcCompat="@drawable/btn_gems_market" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="@string/gems_175"
        android:textColor="#000"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="@+id/imgGem2"
        app:layout_constraintStart_toStartOf="@+id/imgGem2"
        app:layout_constraintTop_toTopOf="@+id/imgGem2" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem2"
        app:layout_constraintEnd_toEndOf="@+id/imgGem2"
        app:layout_constraintStart_toStartOf="@+id/imgGem2"
        app:layout_constraintTop_toTopOf="@+id/imgGem2"
        app:srcCompat="@drawable/gems_mar2" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:fontFamily="@font/regular"
        android:text="@string/tl_6"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem2"
        app:layout_constraintEnd_toEndOf="@+id/imgGem2"
        app:layout_constraintStart_toStartOf="@+id/imgGem2" />

    <ImageView
        android:id="@+id/imgGem3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="@+id/imgGem1"
        app:layout_constraintTop_toBottomOf="@+id/imgGem1"
        app:srcCompat="@drawable/btn_gems_market" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="@string/gems_250"
        android:textColor="#000"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="@+id/imgGem3"
        app:layout_constraintStart_toStartOf="@+id/imgGem3"
        app:layout_constraintTop_toTopOf="@+id/imgGem3" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:fontFamily="@font/regular"
        android:text="@string/tl_9"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem3"
        app:layout_constraintEnd_toEndOf="@+id/imgGem3"
        app:layout_constraintStart_toStartOf="@+id/imgGem3" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem3"
        app:layout_constraintEnd_toEndOf="@+id/imgGem3"
        app:layout_constraintStart_toStartOf="@+id/imgGem3"
        app:layout_constraintTop_toTopOf="@+id/imgGem3"
        app:srcCompat="@drawable/gems_mar3" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem4"
        app:layout_constraintEnd_toEndOf="@+id/imgGem4"
        app:layout_constraintStart_toStartOf="@+id/imgGem4"
        app:layout_constraintTop_toTopOf="@+id/imgGem4"
        app:srcCompat="@drawable/gems_mar4" />

    <ImageView
        android:id="@+id/imgGem4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        app:layout_constraintEnd_toEndOf="@+id/imgGem2"
        app:layout_constraintTop_toBottomOf="@+id/imgGem2"
        app:srcCompat="@drawable/btn_gems_market" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bold"
        android:text="@string/gems_500"
        android:textColor="#000"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="@+id/imgGem4"
        app:layout_constraintStart_toStartOf="@+id/imgGem4"
        app:layout_constraintTop_toTopOf="@+id/imgGem4" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:fontFamily="@font/regular"
        android:text="@string/tl_20"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGem4"
        app:layout_constraintEnd_toEndOf="@+id/imgGem4"
        app:layout_constraintStart_toStartOf="@+id/imgGem4" />
    <com.google.android.gms.ads.AdView
        android:id="@+id/adViewMarket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_test_id"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>